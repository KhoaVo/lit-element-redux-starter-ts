{
  "name": "lit-element-starter-ts",
  "version": "0.0.0",
  "description": "A simple web component",
  "main": "my-element.js",
  "module": "my-element.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf src_compiled",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/* --write",
    "prod": "npm run prod:clean && npm run build && npm run prod:build",
    "prod:watch": "npm run prod:clean && npm run build && npm run prod:build --watch",
    "prod:clean": "rimraf prod",
    "prod:build": "rollup -c --file prod/bundle.js",
    "prod:serve": "es-dev-server --root-dir=prod --app-index dev/index.html --node-resolve --watch",
    "serve": "es-dev-server --app-index dev/index.html --node-resolve --watch",
    "test": "karma start karma.conf.cjs",
    "test:watch": "karma start karma.conf.cjs --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start karma.conf.cjs --update-snapshots",
    "test:prune-snapshots": "karma start karma.conf.cjs --prune-snapshots",
    "checksize": "rollup -c ; cat bundle.js | gzip -9 | wc -c ; rm bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript"
  ],
  "author": "The Polymer Authors",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "lit-element": "^2.4.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.10",
    "@open-wc/testing-karma": "^3.3.11",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.46.1",
    "eslint": "^6.8.0",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "lit-analyzer": "^1.1.10",
    "lit-redux-router": "^0.17.1",
    "mocha": "^7.1.1",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^4.2.3",
    "web-component-analyzer": "^1.0.3"
  }
}
